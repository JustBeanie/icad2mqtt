{
  "name": "ICAD to MQTT Bridge",
  "version": "1.0.0",
  "slug": "icad2mqtt",
  "description": "Fetch active CAD calls from 911events and publish to MQTT",
  "url": "https://github.com/YOUR_USERNAME/icad2mqtt",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "image": "ghcr.io/YOUR_USERNAME/icad2mqtt-{arch}",
  "startup": "service",
  "boot": "auto",
  "hassio_api": false,
  "hassio_role": "default",
  "host_network": false,
  "network_mode": "host",
  "schema": {
    "mqtt_broker": "str",
    "mqtt_topic": "str?",
    "poll_interval": "int?"
  },
  "options": {
    "mqtt_broker": "tcp://localhost:1883",
    "mqtt_topic": "911/cad/events",
    "poll_interval": 30
  },
  "services": {
    "mqtt": {
      "protocol": "tcp"
    }
  },
  "environment": {
    "LOG_LEVEL": "info"
  }
}
